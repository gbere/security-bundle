parameters:
    level: 5
    tmpDir: var/cache/phpstan-tests
    excludes_analyse:
        - tests/bootstrap.php
    autoload_files:
        - vendor/bin/.phpunit/phpunit-8.3-0/vendor/autoload.php
    fileExtensions:
        - php
        - dist
    ignoreErrors:
        -
            # Symfony\Component\Security\Core\Role\Role deprecated
            message: '#Parameter \#4 \$roles of class [a-zA-Z0-9\\_]#'
            path: tests/AccessControlTest.php

includes:
    - vendor/phpstan/phpstan-phpunit/extension.neon
    - vendor/phpstan/phpstan-phpunit/rules.neon
